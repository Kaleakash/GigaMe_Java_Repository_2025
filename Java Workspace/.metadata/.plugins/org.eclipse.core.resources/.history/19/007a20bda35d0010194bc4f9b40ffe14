
public class ArrayExamples {

	public static void main(String[] args) {
	int a;
	int abc[];		// array declaration 
	//System.out.println(abc);
	//System.out.println(a);
	int b=10;
	int xyz[]= {10,20,30,40,50};// array initialization 
	System.out.println(b);
	System.out.println(xyz);	// we get the address 
	System.out.println(xyz[0]);
	System.out.println(xyz[4]);
		//System.out.println(xyz[5]);
	System.out.println(xyz[3]);
	int num[]=new int[10];		//memory creation 
	}

}
